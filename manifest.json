{
   "action": {
      "default_icon": {
         "128": "images/icon.png",
         "16": "images/icon.png",
         "32": "images/icon.png",
         "48": "images/icon.png"
      },
      "default_title": "request mock 插件"
   },
   "background": {
      "service_worker": "scripts/background.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "chromeos": "Ctrl+M",
            "linux": "Ctrl+M",
            "mac": "Command+M",
            "windows": "Ctrl+M"
         }
      }
   },
   "content_scripts": [
      {
         "all_frames": true,
         "js": [
            "scripts/content.js"
         ],
         "matches": [
            "\u003Call_urls>"
         ],
         "run_at": "document_start"
      }
   ],
   "description": "拦截请求并记录请求数据，根据记录一键生成 Mock 规则，配置自定义 Mock 规则，包括 XMLHttpRequest、fetch类接口",
   "host_permissions": [
      "http://*/*",
      "https://*/*"
   ],
   "icons": {
      "128": "images/icon.png",
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png"
   },
   "manifest_version": 3,
   "name": "MockSnap",
   "options_page": "options/index.html",
   "permissions": [
      "storage",
      "declarativeNetRequest"
   ],
   "version": "1.0.0",
   "web_accessible_resources": [
      {
         "matches": [
            "\u003Call_urls>"
         ],
         "resources": [
            "scripts/override.js",
            "scripts/recorder.js"
         ]
      }
   ]
}